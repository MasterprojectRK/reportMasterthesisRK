@Article{Zufferey2018,
  author     = {Marie Zufferey and Daniele Tavernari and Elisa Oricchio and Giovanni Ciriello},
  title      = {Comparison of computational methods for the identification of topologically associating domains},
  doi        = {10.1186/s13059-018-1596-9},
  number     = {1},
  volume     = {19},
  abstract   = {Overview and extensive comparison of 22 TAD detection tools, including Hi-C explorer.
This is insofar relevant for the thesis, as we try to penalize structural mismatches, which could be done by using a custom loss function based on TAD detection algorithms.
Supplementary material 1 has nice short descriptions how the different algorithms work. 
The tools can be grouped into four general approaches: Binning and linear score, clustering, statistical and graph-based. Most seem too difficult to implement on tensors and/or might take too long to compute. Some also have a large number of parameters, up to 13, making them difficult to tune.
One of the assessment features was concordance with biological features, given here by CTCF and Cohesin subcomponents enrichment. In this regard, and combined with computational efficiency, TopDom, arrowhead and GMAP performed well.
TopDom computes mean interaction frequencies within diamonds and then evaluates minima in the statistics for TAD boundary or not.
Arrowhead uses a special transformation of the Hi-C matrix (should be possible with Tensors, see Rao2014) and then computes three scores on that using DP, taking bins with max. score as boundaries. GMAP is a statistical approach based on a 2-component Gaussian modeling interaction frequencies. It has several parameters.},
  file       = {fulltext:zufferey2018_comparisonTadDetection.pdf:PDF;descriptionOfTools:zufferey2018_supp1.pdf:PDF},
  groups     = {partially relevant, LossFunctions},
  journal    = {Genome Biology},
  keywords   = {read},
  month      = {dec},
  publisher  = {Springer Science and Business Media {LLC}},
  readstatus = {read},
  year       = {2018},
}

